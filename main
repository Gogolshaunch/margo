startTime = 0


def chat_margo(note):
    response = g4f.ChatCompletion.create(
        model=g4f.models.gpt_4,
        messages=[{"role": "user", "content": note}])
    return response


def speak(text):
    va_voice.speak(text)


def va_respond(voice: str):
    if voice.startswith(configuration.name):
        cmd = recognize_cmd(filter_cmd(voice))

        if cmd['cmd'] not in configuration.comands.keys():
            speak("–ß—Ç–æ?")
        else:
            execute_cmd(cmd['cmd'], voice)


def filter_cmd(raw_voice: str):
    cmd = raw_voice

    for x in configuration.name:
        cmd = cmd.replace(x, "").strip()

    return cmd


def recognize_cmd(cmd: str):
    rc = {'cmd': '', 'percent': 0}
    for c, v in configuration.comands.items():

        for x in v:
            vrt = fuzz.ratio(cmd, x)
            if vrt > rc['percent']:
                rc['cmd'] = c
                rc['percent'] = vrt

    return rc


def execute_cmd(cmd, voice):
    global startTime
    if cmd == 'chat_gpt':
        try:
            speak(chat_margo(voice))
            print(chat_margo(voice))
        except:
            speak('–Ø —Ç–∞–∫ –µ—â–µ –Ω–µ —É–º–µ—é, –∏–∑–≤–∏–Ω–∏—Ç–µ')
